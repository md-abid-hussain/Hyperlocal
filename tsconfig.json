{
  "compilerOptions": {
    "target": "es2021",
    "module": "commonjs",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,          
    "skipLibCheck": true,
    "sourceMap": true
  },
  "exclude": ["node_modules", "dist"],
  "include": ["src/**/*.ts"]
}
